{
    "name": "Tab Title Picker",

    "description": "It fetches and display the current tabs's title.",

    "version": "1.0",

    "manifest_version": 3,

    "permissions": ["tabs"],

    "action": {
        "default_popup": "popup.html"
    },
    
    "icons": {
        "16": "/images/picker_16.png",
        "32": "/images/picker_32.png",
        "48": "/images/picker_48.png"
    }
}

